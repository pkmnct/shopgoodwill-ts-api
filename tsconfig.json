{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["es2015", "es2016", "es2017", "es2018", "es2021", "DOM"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true
  },
  "include": ["./"],
  "exclude": []
}
